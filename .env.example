# User persona (standard_user, problem_user, performance_glitch_user, locked_out_user)
SWAG_USERNAME=standard_user

# Set to 1 to enable random data generation for checkout fields
FUZZ_DATA=0

# Automatically set to true in GitHub Actions / CI environments
CI=false

# OpenAI API Key (required for agentic automation)
OPENAI_API_KEY=your_openai_api_key_here

# Model selection (optional, defaults to gpt-4o)
OPENAI_MODEL=gpt-4o